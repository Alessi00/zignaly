{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "esnext",
    "allowJs": true,
    "checkJs": true,
    "alwaysStrict": true,
    "strictNullChecks": false,
    "declaration": false,
    "emitDeclarationOnly": false,
    "outDir": "types",
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "removeComments": false
  },
  "include": ["./src/**/*"],
}